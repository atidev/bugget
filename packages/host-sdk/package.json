{
  "name": "@bugreport/host-sdk",
  "version": "1.0.0",
  "description": "SDK for Bugreport host applications to manage extensions",
  "type": "module",
  "exports": {
    "./init": {
      "types": "./dist/init.d.ts",
      "default": "./dist/init.js"
    },
    "./loader": {
      "types": "./dist/loader.d.ts",
      "default": "./dist/loader.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "bugreport",
    "host",
    "extensions",
    "loader"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "axios": "^1.0.0",
    "effector": "^22.0.0 || ^23.0.0",
    "effector-react": "^22.0.0 || ^23.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "react-router-dom": "^6.0.0 || ^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "axios": "^1.7.0",
    "effector": "^23.0.0",
    "effector-react": "^23.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.1.9"
  }
}
